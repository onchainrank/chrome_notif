{
    "manifest_version": 3,
    "name": "onchainrank notifier",
    "version": "1.6",
    "description": "Connects to the onchainrank server, displays connection status, counts notify events, and handles API key configuration.",
    "icons": {
        "16": "ocr-blue-16.png",
        "48": "ocr-blue-48.png",
        "128": "ocr-blue-128.png"
    },
    "options_page": "options.html",
    "host_permissions": [
        "https://api.onchainrank.com/*"
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "ocr-blue-16.png",
            "48": "ocr-blue-48.png",
            "128": "ocr-blue-128.png"
        },
        "default_title": "onchainrank notifier"
    }
}